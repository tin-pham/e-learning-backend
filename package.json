{
  "name": "school-media-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "reset": "pnpm run down && sudo rm -rdf data && pnpm run up && pnpm run app-run migration ",
    "up": "docker compose -f ./docker-compose.dev.yml up -d",
    "down": "docker compose -f ./docker-compose.dev.yml down",
    "app": "docker logs school-dev --follow",
    "app-run": "docker exec -it school-dev npm run",
    "migration": "ts-node -r tsconfig-paths/register ./src/database/run-migration.ts",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config ./test/unit/jest.unit.ts",
    "test:watch": "jest --watch",
    "test:cov": "jest --config ./test/unit/jest.unit.ts --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/e2e/jest.e2e.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.485.0",
    "@elastic/elasticsearch": "^7.17.13",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/cache-manager": "^2.1.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/elasticsearch": "^10.0.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.3.7",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/swagger": "^7.1.16",
    "@nestjs/terminus": "^10.2.0",
    "@nestjs/throttler": "^5.0.1",
    "@nestjs/websockets": "^10.3.7",
    "@types/passport-jwt": "^3.0.13",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^5.3.1",
    "cache-manager-memcached-store": "^5.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cron": "^3.1.7",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.27.0",
    "memcache-pp": "^0.3.3",
    "moderndash": "^3.9.0",
    "multer": "1.4.5-lts.1",
    "nanoid": "^3.3.7",
    "nestjs-form-data": "^1.9.5",
    "nestjs-kysely": "^0.1.6",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@jest-performance-reporter/core": "^2.1.3",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cache-manager-memcached-store": "^3.0.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "jest-junit": "^16.0.0",
    "jest-sonar": "^0.2.16",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
